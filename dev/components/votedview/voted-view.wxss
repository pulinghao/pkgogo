.vote-text-container {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-top: 22rpx;
  }

  .left-voted-label, .left-text-voted {
    font-size: 22rpx;
    color: #3392FF;
  }

  .left-voted-label {
    margin-left: 6rpx;
  }

  .right-voted-label {
    margin-left: 6rpx;
  }
  
  .right-voted-label, .right-text-voted {
    font-size: 22rpx;
    color: #FF3363;
  }

  .win-cup {
    width: 34rpx;
    height: 36rpx;
    margin-left: 10rpx;
  }
  
  .vote-rate-container {
    width: 100%;
    height: 50rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    margin-top: 10rpx;
  }
  
  .left-part {
    width: 50%;
    height: 26rpx;
    background: #ebebeb;
    display: flex;
    flex-direction: row;
    justify-content: flex-end;
    border-top-left-radius: 17rpx;
    border-bottom-left-radius: 17rpx;
  }
  
  .left-part-bar {
    height: 100%;
    background: #4a93ff;
    border-top-left-radius: 17rpx;
    border-bottom-left-radius: 17rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
  }
  
  .right-part-bar {
    height: 100%;
    background: #FF3363;
    border-top-right-radius: 17rpx;
    border-bottom-right-radius: 17rpx;
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
  }
  .right-part {
    width: 50%;
    height: 26rpx;
    background: #ebebeb;
    display: flex;
    flex-direction: row;
    justify-content: flex-start;
    border-top-right-radius: 17rpx;
    border-bottom-right-radius: 17rpx;
  }

  .left-text-container {
    display: flex;
    flex-direction: row;
    align-items: center;
  }

  .vote-vs-icon {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    left: 47%;
  }

  .voted-label {
    margin-left: 6rpx;
  }

  .total {
    color: #FFFFFF;
    font-size: 22rpx;
  }