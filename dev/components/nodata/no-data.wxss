/* components/nodata/no-data.wxss */
.container{
    width: 100%;
    /*height: 1400rpx;*/
    background-color: white;
    display: flex;
    flex-direction: column;
    justify-content: flex-start;
}
.img-container{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-top: 50rpx;
}
.img-style{
    width: 290rpx;
    height: 282rpx;
}
.text-container{
    text-align: center;
    margin-top: 25rpx;
}
.text-style{
    font-size:26rpx;
    font-family:PingFangTC-Regular;
    font-weight:400;
    color:rgba(140,140,140,1);
}
.button-container{
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    margin-top: 50rpx;
}
.button-style{
    background-color: white;
    width:280rpx;
    height:60rpx;
    border-radius:4rpx;
    border:2rpx solid rgba(140,140,140,0.6);
    font-size:26rpx;
    font-family:PingFangTC-Regular;
    font-weight:400;
    color:rgba(50,50,50,1);
    line-height: 60rpx;
    text-align: center;
}
